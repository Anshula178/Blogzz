{
    "version": 2,
    "name": "fullstack-blog-app",
    "builds": [
      {
        "src": "Frontend/package.json",
        "use": "@vercel/static-build",
        "config": {
          "distDir": "build"
        }
      },
      {
        "src": "Backend/index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      { "src": "/api/(.*)", "dest": "Backend/index.js" },
      { "src": "/(.*)", "dest": "/Frontend/$1" }
    ]
  }
  